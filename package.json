{
  "name": "rtm-dashboard",
  "version": "0.5.7",
  "author": "gustaveWPM (github.com/gustaveWPM)",
  "contributors": [
    "Tirraa (github.com/Tirraa)"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "cross-env BLOG_POSTS_FOLDER=./posts I18N_LOCALES_SCHEMA=./src/i18n/locales/schema.ts pnpm --filter @rtm/prebuilder start",
    "build": "next build",
    "postbuild": "next-sitemap",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "test-before-build": "vitest --run -t '^((?!\\[POSTBUILD\\]).)*$'",
    "test-after-build": "vitest --run postbuild",
    "pretest": "next build",
    "test": "vitest --run",
    "coverage": "vitest --run --coverage.enabled --coverage.all=false",
    "coverage-all": "vitest --run --coverage",
    "only-tests": "vitest --run",
    "prepare": "husky install",
    "commit": "git-cz",
    "format": "prettier --log-level warn --write .",
    "ts-prune": "ts-prune --ignore '^.(next|contentlayer|vitest)' | .ts-prune/sanitize.py --quiet",
    "ts-prune-verbose": "ts-prune --ignore '^.(next|contentlayer|vitest)' | .ts-prune/sanitize.py",
    "ci:vercel-build-command": "pnpm test-before-build && pnpm build && pnpm test-after-build",
    "ci:lint": "eslint . --max-warnings 0",
    "ci:typecheck": "tsc --noEmit --pretty",
    "ci:format-check": "prettier . --check"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "^14.0.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-hook/media-query": "^1.1.1",
    "@rtm/shared-lib": "*",
    "@rtm/shared-types": "*",
    "@vercel/speed-insights": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "github-slugger": "^2.0.0",
    "hastscript": "^8.0.0",
    "inflection": "^2.0.1",
    "intl-locale-textinfo-polyfill": "^2.1.1",
    "lucide-react": "^0.290.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-contentlayer": "^0.3.4",
    "next-international": "1.1.4",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-paginate": "8.1.3",
    "react-spinners": "^0.13.8",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-external-links": "^3.0.0",
    "rehype-pretty-code": "^0.10.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^0.14.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/cz-commitlint": "^18.2.0",
    "@transloadit/ts-fly": "^0.1.4",
    "@tsd/typescript": "^5.3.2",
    "@types/hast": "^3.0.2",
    "@types/inflection": "^1.13.0",
    "@types/node": "20.5.0",
    "@types/nprogress": "^0.2.0",
    "@types/prop-types": "^15.7.11",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "10.4.15",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "editorconfig": "^2.0.0",
    "eslint": "8.47.0",
    "eslint-config-next": "^13.5.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "madge": "^6.1.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.2.0",
    "tsd": "^0.29.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.9",
    "vitest": "1.0.4"
  },
  "engines": {
    "node": "18.x"
  },
  "packageManager": "^pnpm@8.12.0"
}
