{
  "name": "rtm-dashboard",
  "version": "0.7.4",
  "author": "gustaveWPM (github.com/gustaveWPM)",
  "contributors": [
    "Tirraa (github.com/Tirraa)"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "prebuild": "pnpm --filter @rtm/prebuilder start --blog-posts-folder ./content/blog --i18n-locales-schema ./src/i18n/locales/schema.ts --lp-folder ./content/landing-pages --pages-folder ./content/pages --pretty-codegen",
    "build": "next build",
    "postbuild": "next-sitemap",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "test": "make test",
    "test:run": "vitest --run",
    "mutations-tests": "make mutations-tests",
    "mutations-tests:run": "stryker run",
    "before-build:tricky-typechecks": "tsc --noEmit --skipLibCheck --pretty ./packages/prebuilder/src/generators/pages/__tests__/prebuild.pagesType.test.ts",
    "before-build:test": "vitest --run prebuild",
    "after-build:test": "vitest --run postbuild",
    "coverage": "vitest --run --coverage.enabled --coverage.all=false",
    "coverage-all": "vitest --run --coverage",
    "commit": "git-cz",
    "format": "prettier --log-level warn --write .",
    "bundle-analyzer": "cross-env ANALYZE=true next build",
    "check-coding-style": "make check-coding-style",
    "ts-prune": "ts-prune --ignore '^.(next|contentlayer|vitest|stryker-tmp)' | .ts-prune/sanitize.py --quiet",
    "ts-prune-verbose": "ts-prune --ignore '^.(next|contentlayer|vitest|stryker-tmp)' | .ts-prune/sanitize.py",
    "ci:vercel-build-command": "make before-build:tricky-typechecks && make before-build:test && make && make after-build:test",
    "ci:lint": "eslint . --max-warnings 0",
    "ci:typecheck": "tsc --noEmit --pretty",
    "ci:format-check": "prettier . --check",
    "vitest:ui": "vitest --ui"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "^14.0.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-hook/media-query": "^1.1.1",
    "@rtm/shared-lib": "*",
    "@rtm/shared-types": "*",
    "@vercel/speed-insights": "^1.0.2",
    "bentocache": "1.0.0-beta.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^3.3.1",
    "github-slugger": "^2.0.0",
    "inflection": "^3.0.0",
    "intl-locale-textinfo-polyfill": "^2.1.1",
    "lucide-react": "^0.331.0",
    "mobile-detect": "^1.4.5",
    "next": "^14.1.0",
    "next-auth": "^4.24.6",
    "next-contentlayer": "^0.3.4",
    "next-international": "1.2.4",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "react-paginate": "8.1.3",
    "react-spinners": "^0.13.8",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-external-links": "^3.0.0",
    "rehype-pretty-code": "^0.13.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^1.1.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/cz-commitlint": "^18.2.0",
    "@stryker-mutator/api": "^8.2.2",
    "@stryker-mutator/core": "^8.2.2",
    "@stryker-mutator/typescript-checker": "^8.2.2",
    "@stryker-mutator/vitest-runner": "^8.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@transloadit/ts-fly": "^0.1.4",
    "@tsd/typescript": "^5.3.2",
    "@types/hast": "^3.0.2",
    "@types/inflection": "^1.13.0",
    "@types/node": "^20.5.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "editorconfig": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^13.3.8",
    "husky": "^9.0.11",
    "jest-tsd": "0.2.2",
    "lint-staged": "^15.0.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "ts-prune": "0.10.3",
    "typescript": "^5.3.2",
    "vite": "^5.0.12",
    "vitest": "1.2.2"
  },
  "engines": {
    "node": "18.x"
  },
  "packageManager": "^pnpm@8.15.3"
}
